#listview {
	layout:		vertical;
	spacing:	4px;
	lines:		8;
}

#entry {
	padding:	4px 4px;
	expand:		false;
	width:		10em;
	font:		"Hasklig";
}

#element {
	padding:	4px 14px;
}

#element selected {
	background-color:	#f0a43a;
}

