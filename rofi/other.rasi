* {
	background-color:	#f2e6d1;
	border-color:		#70655b;
	text-color:		#802152;
	font:		"Hasklig 13";
}

#configuration {
	modi:		"window";
	window-format:	"{w} || {n}";
	width:		50;
	window-command:	 "xkill -id {window}";

	hide-scrollbar:	false;
	show-icons:	false;
	/* drun-icon-theme: gruvbox-soft; */
	cycle:	false;
}

